{
    "name": "nateinaction/wordpress-integration",
    "description": "Docker environment to run WordPress integration tests.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nate Gay",
            "email": "email@nategay.me"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress/wordpress",
                "version": "4.9.8",
                "dist": {
                    "url": "https://github.com/WordPress/wordpress-develop/archive/4.9.8.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
    	"phpunit/phpunit": "^5",
        "wordpress/wordpress": "4.9.8"
    }
}
